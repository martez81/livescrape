{
  "manifest_version": 2,

  "name": "LiveScrape Chrome Ext.",
  "description": "This extension allows to setup scraper.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Create LiveScrape parser."
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}